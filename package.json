{
	"name": "neural-network",
	"version": "1.0.0",
	"description": "Toy Neural Network in TypeScript with Examples",
	"keywords": [
		"ai",
		"artificial-intelligence",
		"machine-learning",
		"neural-network",
		"typescript"
	],
	"homepage": "https://github.com/rorazliev/neural-network#readme",
	"bugs": {
		"url": "https://github.com/rorazliev/neural-network/issues"
	},
	"license": "MIT",
	"author": {
		"name": "Ruslan Orazliev",
		"url": "https://linkedin.com/in/rorazliev"
	},
	"main": "build/index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/rorazliev/neural-network.git"
	},
	"scripts": {
		"build": "tsc",
		"lint": "eslint --ext .js,.ts --fix",
		"lint:check": "eslint --ext .js,.ts",
		"format": "prettier . --write",
		"format:check": "prettier . --check",
		"types:check": "tsc --pretty --noEmit",
		"check:all": "npm run lint:check && npm run format:check && npm run types:check",
		"test": "jest --watch",
		"test:ci": "jest --ci",
		"prepare": "husky install"
	},
	"dependencies": {
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.21.0",
		"@babel/core": "^7.21.3",
		"@babel/preset-env": "^7.20.2",
		"@babel/preset-typescript": "^7.21.0",
		"@types/jest": "^29.4.4",
		"@types/node": "^18.15.3",
		"@types/uuid": "^9.0.1",
		"@typescript-eslint/eslint-plugin": "^5.55.0",
		"@typescript-eslint/parser": "^5.55.0",
		"eslint": "^8.36.0",
		"eslint-config-google": "^0.14.0",
		"eslint-config-prettier": "^8.7.0",
		"husky": "^8.0.0",
		"jest": "^29.5.0",
		"prettier": "^2.8.4",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5"
	}
}
